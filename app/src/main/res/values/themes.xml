<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.1201766_1201086_CourseProject" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.YourApp" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#8A9C6B</item> <!-- ActionBar background color -->
        <item name="colorPrimaryDark">#8A9C6B</item> <!-- Status bar color -->
        <item name="colorAccent">#8A9C6B</item> <!-- Accent color -->
    </style>

    <style name="Theme.MyApp.Light" parent="Theme.MaterialComponents.DayNight">
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:colorBackground">@color/background_color</item>
    </style>


</resources>